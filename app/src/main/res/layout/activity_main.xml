            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/vg_top"
            android:background="@color/black_trans"
            android:minWidth="240.0dip"
            android:orientation="vertical"
            android:visibility="gone" >

            <LinearLayout
                android:id="@id/vg_result_control_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.0"
                android:orientation="horizontal"
                android:visibility="gone" >

                <Button
                    android:id="@id/btn_pre_info"
                    style="@style/txt_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:text="上一条" />

                <Button
                    android:id="@id/btn_next_info"
                    style="@style/txt_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:text="下一条" />
            </LinearLayout>

            <LinearLayout
                android:id="@id/vg_layer_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.0"
                android:orientation="horizontal" >

                <TextView
                    android:id="@id/txt_title"
                    style="@style/txt_menu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
